{
  "name": "ᴀɴᴏɴ ダ ᴍᴜsɪᴄ",
  "description": "A Telegram Music Player Bot, written in Python with Pyrogram and Py-Tgcalls.",
  "logo": "https://te.legra.ph/file/b8a0c1a00db3e57522b53.jpg",
  "keywords": [
    "python3",
    "telegram",
    "bot",
    "AnonX",
    "MusicBot",
    "telegram-bot",
    "pyrogram"
  ],
  "env": {
    "API_ID": {
      "description": "Get this value from https://my.telegram.org",
      "required": true
    },
    "API_HASH": {
      "description": "Get this value from https://my.telegram.org",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A Bot's token from Botfather",
      "required": true
    },
    "MONGO_DB_URI": {
      "description": "MongoDB URI from cloud.mongodb.com",
      "required": true
    },
    "OWNER_ID": {
      "description": "User ID of the owner",
      "required": true
    },
    "STRING_SESSION": {
      "description": "A Pyrogram String Session from @StringFatherBot",
      "required": true
    },
    "HEROKU_API_KEY": {
      "description": "Your Heroku account API key",
      "required": false
    },
    "HEROKU_APP_NAME": {
      "description": "Your Heroku app name",
      "required": false
    },
    "LOGGER_ID": {
      "description": "Log Group ID (not a channel)",
      "required": true
    }
  },
  "buildpacks": [
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
    },
    {
      "url": "heroku/python"
    }
  ],
  "stack": "heroku-22"
}